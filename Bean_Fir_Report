
public class Bean_Fir_Report {

    int firno;
    String firreport;

    public Bean_Fir_Report() {
    }

    public Bean_Fir_Report(int firno, String firreport) {
        this.firno = firno;
        this.firreport = firreport;
    }

    public int getFirno() {
        return firno;
    }

    public String getFirreport() {
        return firreport;
    }

    public void setFirno(int firno) {
        this.firno = firno;
    }

    public void setFirreport(String firreport) {
        this.firreport = firreport;
    }

}
